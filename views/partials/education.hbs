{{#if resume.education.length}}
<section>
	<h2>Education</h2>
	{{#each resume.education}}
        {{#if area}}
            {{> section-item title=institution website=website subtitle=(concat studyType ' in ' area) startDate=startDate endDate=endDate text=summary keywords=courses}}
        {{else}}
            {{> section-item title=institution website=website subtitle=studyType startDate=startDate endDate=endDate text=summary keywords=courses}}
        {{/if}}
	{{/each}}
</section>
{{/if}}
